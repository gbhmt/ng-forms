root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{js,ts,json,html}]
indent_style = tab
indent_size = 4

[{package,package-lock,tsconfig,tsconfig.*,tslint}.json]
indent_style = space
indent_size = 2

[.vscode/*.json]
indent_style = space
indent_size = 2
